Algoritmo "operadores"

Var
   a: real
   b: real


Inicio
      a <- 5
      b <- 2
    escreva ("Soma ",a+b)
    escreva ("Subtracao ",a-b)
    escreva ("Multiplicacao ",a*b)
    escreva ("Divisao ",a/b)
    escreva ("Divisão inteira ",a\b)
    escreva ("Exponenciacao ",a^b)
    escreva ("Resto da divisao ",a%b)


Fimalgoritmo