Algoritmo "ex001"

Var
  anoNascimento: inteiro;
  anoAtual:inteiro;
  idade:inteiro;
  
Inicio
      escreva("Em que ano estamos: ")
      leia(anoAtual)
      escreva("Indique o ano em que nasceu:")
      leia(anoNascimento)
      idade <- anoAtual-anoNascimento
      escreval("A Minha idade sera: ", idade, " anos")

Fimalgoritmo