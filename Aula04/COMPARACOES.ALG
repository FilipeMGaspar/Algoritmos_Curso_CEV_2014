Algoritmo "comparacoes"

Var
a, b, c : inteiro


Inicio
  a <- 2
  b <- 3
  c <- 5
  
  escreval (a, " > " ,b, a>b)
  escreval (a, " = ", b, a=b)
  escreval (a, " e diferente de ", b ,a<>b)
  escreval (a>2)
  escreval (a>=2)
  escreval (c=a+b)
  escreval (c>=b^a)
  escreval (c<=b^a)
  escreval (b<=c%2)
  escreval (1=c%2)
  

Fimalgoritmo