Algoritmo "utilizadormandacontar"

Var
   valor, contador : inteiro


Inicio
    escreva("Deseja contar até quanto? ")
    leia(valor)
    contador <-0
    enquanto (contador <= valor) faca

       contador <- contador + 1
       escreval(contador)
       
    fimenquanto
    escreval()
    escreval("Terminei de contar!")

Fimalgoritmo