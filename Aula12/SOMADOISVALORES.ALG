Algoritmo "somadoisvalores"

Var
  x, y: inteiro

  procedimento soma(a, b: inteiro)
    inicio
      escreval
      escreval("Recebi o valor ", a)
      escreval("Recebi o valor ", b)
      escreval("A soma entre os dois", a + b)
  fimprocedimento

Inicio
   x <- 5
   y <- 3
   
   soma(x,y)


Fimalgoritmo